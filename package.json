{
	"name": "academic-history-calculator",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "npm run generate:ogp && next build",
		"start": "next start",
		"lint": "biome check --write .",
		"format": "biome format --write .",
		"check": "biome check .",
		"typecheck": "tsc --noEmit",
		"validate": "npm run typecheck && npm run check",
		"generate:ogp": "npx ts-node --esm scripts/generate-ogp.ts",
		"clean": "Remove-Item -Recurse -Force .next, out, node_modules/.cache -ErrorAction SilentlyContinue"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"@vercel/speed-insights": "^1.2.0",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.4",
		"next": "^16.0.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/jspdf": "^1.3.3",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"autoprefixer": "^10.4.22",
		"postcss": "^8.5.6",
		"puppeteer": "^24.31.0",
		"sharp": "^0.34.5",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3"
	}
}
